pragma solidity >=0.7.0 <0.9.0;

contract Interpreter {
    constructor() payable {
        // Copy the interpreter bytecode to memory
        bytes memory runtime = INTERPRETER_BYTECODE;

        assembly {
            // Store the contract address in the bytecode, this is useful to prevent someone
            // from destroy it, once the opcodes SELFDESTRUCT and DELEGATECALL are disabled
            // when `address(this)` matches the address in the bytecode, if you want to execute
            // those opcodes, you must use DELEGATECALL.
            let offset := add(runtime, 0x200f)
            let chunk := and(mload(offset), 0xffffffffffffffffffffffff)
            chunk := or(chunk, shl(0x60, address()))
            mstore(offset, chunk)

            // Return the interpreter bytecode
            return(add(runtime, 32), mload(runtime))
        }
    }

    bytes constant private INTERPRETER_BYTECODE = hex""; 
}
